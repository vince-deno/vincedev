                         <script>
                            let vio=document.querySelector('.progress-value')
                            let vie=document.querySelector('.vlmprogress')
            
                            vio.onclick=function(){
                                vie.classList.toggle('active')
                            }
            
                            function vlmscroll(){
                                let s=document.querySelector('.vlmprogress')
                                let sc=document.querySelector('.progress-value')
                                let top=document.querySelector('.vlmtop')
                                let p=document.documentElement.scrollTop;
                                let hs=document.documentElement.scrollHeight-document.documentElement.clientHeight;
                                let sh=Math.round((p*100)/hs)
            
                                if (p>50) {
                                    s.style.display="block"
                                }else{
                                    s.style.display="none"
                                }
                                top.addEventListener("click",()=>{
                                    document.documentElement.scrollTop=0;
                                })
                                sc.style.background =`conic-gradient(#1b2b3b ${sh}%,cyan ${sh}%)`
                            }
                            window.onscroll = vlmscroll
                            // window.onload= vlmload
            
                         </script>
